 -->
<Modules>

<ModuleRegistry modname="dsp_blocks_2"
                modulePath="$MATLABROOT/bin/$ARCH"
                registryVersion="1.0"
                >
<BlockRegistration block_type_str = "DiscreteFir" />
<BlockRegistration block_type_str = "DiscreteFilter"/>
<BlockRegistration block_type_str = "DiscreteTransferFcn"/>
<BlockRegistration block_type_str = "TimeScope"/>
<BlockRegistration block_type_str = "SpectrumAnalyzer"/>
<BlockRegistration block_type_str = "AllpoleFilter"/>

</ModuleRegistry>

<ModuleRegistry modname="matlabsystemblock"
                modulePath="$MATLABROOT/bin/$ARCH"
                registryVersion="1.0"
                >
<BlockRegistration block_type_str = "MATLABSystem" />

</ModuleRegistry>

</Modules>
